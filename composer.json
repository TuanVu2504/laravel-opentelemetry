{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": "^2.6",
        "laravel/framework": "^9.19",
        "laravel/octane": "^1.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "open-telemetry/api": "1.0.3",
        "open-telemetry/context-swoole": "^1.0",
        "open-telemetry/exporter-otlp": "1.0.3",
        "open-telemetry/opentelemetry-auto-laravel": "0.0.22",
        "open-telemetry/opentelemetry-auto-psr15": "^1.0",
        "open-telemetry/opentelemetry-auto-psr18": "^1.0",
        "open-telemetry/sdk": "1.0.8",
        "open-telemetry/transport-grpc": "^1.0",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "swoole/ide-helper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
